    "((?:<!-- *)?(?:#|//|/\\*+|<!--) *DONE:?)(?!\\w)((?: +[^\n@]+?)(?= *(?://|/\\*+|<!--|@))|(?: +[^@\n]+)?)": [
      {
        "overviewRulerColor": "#090",
